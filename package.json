  "license": "MIT",
  "devDependencies": {
    "nodemon": "^1.14.6"
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "concurrently": "^3.5.1",
    "cors": "^2.8.5",
    "express": "^4.16.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.10.2",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "recompose": "^0.30.0",
    "validator": "^13.1.1"
  }
}
